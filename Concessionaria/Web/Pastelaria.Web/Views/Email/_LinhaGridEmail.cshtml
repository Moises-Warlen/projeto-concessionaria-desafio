@model Concessionaria.Web.Application.Email.Model.EmailModel
@{
    Layout = null;
}

<tr data-id="@Model.IdEmail">
    <td><i class="fas fa-map-marker-alt"></i></td>
    <td data-label="email" data-name="Email">@Model.Email</td>
    <td data-label="">
        <a class="btn btn-danger btn-sm" onclick="clientes.deletarEmail(this)" title="Excluir email">
            <i class="fas fa-trash-alt"></i>
        </a>
    </td>
</tr>
