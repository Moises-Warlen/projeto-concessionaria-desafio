@model Concessionaria.Web.Application.Telefone.Model.TelefoneModel
@{
    Layout = null;
}

<tr data-id="@Model.IdTelefone" >
    <td><i class="fas fa-phone"></i></td>
    <td data-label="ddd" data-name="DDD">@Model.DDD</td>
    <td data-label="numero" data-name="Numero">@Model.Numero</td>
    <td data-label="tipo" data-name="Tipo">@Model.Tipo</td>
    <td data-label="">
        <a class="btn btn-danger btn-sm" onclick="clientes.deletarTelefone(this)" title="Excluir telefone">
            <i class="fas fa-trash-alt"></i>
        </a>
    </td>
</tr>